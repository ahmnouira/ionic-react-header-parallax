{
  "name": "ionic-react-header-parallax",
  "version": "0.0.4",
  "description": "A React Hook parallax effect for Ionic React <IonHeader> component",
  "author": {
    "name": "Ahmd Nouira",
    "email": "ahmnouira@gmail.comm"
  },
  "repository": {
    "type": "git",
